				<string>&lt;html&gt;
&lt;head&gt;
  &lt;meta charset="UTF-8" /&gt;
  &lt;meta name="viewport" content="width=device-width, initial-scale=1.0" /&gt;
  &lt;meta name="color-scheme" content="light dark" /&gt;
  &lt;style&gt;
    body {
      margin: 0;
      padding: 0;
      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
      background-color: #f4f4f4;
      color: #333;
      display: flex;
      justify-content: center;
      align-items: center;
      height: 100vh;
      text-align: center;
    }

    .container {
      background: #fff;
      padding: 2rem;
      border-radius: 12px;
      box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
      width: 90%;
      max-width: 400px;
      position: relative;
    }

    h1 {
      margin-bottom: 1rem;
      font-size: 2rem;
    }

    input[type="color"] {
      width: 100px;
      height: 100px;
      margin: 1rem 0;
      cursor: pointer;
    }

    ul {
      text-align: left;
      margin: 1rem 0;
      padding-left: 1.5rem;
    }

    li {
      font-size: 1rem;
      color: #555;
      line-height: 1.6;
    }

    #hexWrapper {
      position: relative;
      display: inline-flex;
      align-items: center;
      gap: 0.5rem;
      margin-top: 0.5rem;
    }

    #hexDisplay {
      font-family: monospace;
      font-size: 1.25rem;
      padding: 0.5rem 1rem;
      background: #e0e0e0;
      border-radius: 6px;
      cursor: pointer;
      display: inline-block;
    }

    #tooltip {
      color: #555;
      font-size: 0.9rem;
      font-weight: 500;
      white-space: nowrap;
      opacity: 1;
      transition: opacity 0.4s ease;
      pointer-events: none;
    }

    #tooltip.hide {
      opacity: 0;
    }
  &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
  &lt;div class="container"&gt;
    &lt;h1&gt;Color Picker&lt;/h1&gt;
    &lt;ul&gt;
      &lt;li&gt;&lt;strong&gt;Step 1:&lt;/strong&gt; Pick a color ‚Äî the hex code will appear below.&lt;/li&gt;
      &lt;li&gt;&lt;strong&gt;Step 2:&lt;/strong&gt; Tap the hex code to copy it to your clipboard.&lt;/li&gt;
      &lt;li&gt;&lt;strong&gt;Step 3:&lt;/strong&gt; Tap Done.&lt;/li&gt;
    &lt;/ul&gt;
    &lt;input type="color" id="colorInput"&gt;
    &lt;div id="hexWrapper"&gt;
      &lt;div id="hexDisplay" tabindex="0"&gt;#000000&lt;/div&gt;
      &lt;div id="tooltip"&gt;üìã Copy&lt;/div&gt;
    &lt;/div&gt;
  &lt;/div&gt;

  &lt;script&gt;
    const colorInput = document.getElementById('colorInput');
    const hexDisplay = document.getElementById('hexDisplay');
    const tooltip = document.getElementById('tooltip');

    function showTemporaryMessage(message) {
      tooltip.textContent = message;
      tooltip.classList.remove('hide');

      setTimeout(() =&gt; {
        tooltip.classList.add('hide');
        setTimeout(() =&gt; {
          tooltip.textContent = 'üìã Copy';
          tooltip.classList.remove('hide');
        }, 400);
      }, 2000);
    }

    colorInput.addEventListener('input', () =&gt; {
      const hex = colorInput.value.toUpperCase();
      hexDisplay.textContent = hex;
    });

    hexDisplay.addEventListener('click', () =&gt; {
      navigator.clipboard.writeText(hexDisplay.textContent).then(() =&gt; {
        showTemporaryMessage('‚úîÔ∏è Copied');
      });
    });
  &lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</string>
