        {
            "WFWorkflowActionIdentifier": "is.workflow.actions.comment",
            "WFWorkflowActionParameters": {
                "WFCommentActionText": "This shortcut lets you to choose a color from the system color picker, then presents its name and corresponding values in HEX, RGB, CMYK, and HSV/HSL formats.\n‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ\nüôè Modified from:\n‚Ä¢ https://www.reddit.com/r/shortcuts/comments/v1712k/if_you_ever_need_a_quick_color_picker_for_your\n‚Ä¢ https://www.reddit.com/r/shortcuts/comments/1aqn3c0/converting_hex_color_codes_to_names"
            }
        },
        {
            "WFWorkflowActionIdentifier": "is.workflow.actions.gettext",
            "WFWorkflowActionParameters": {
                "WFTextActionText": "<html>\n<head>\n  <meta charset=\"UTF-8\" />\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n  <meta name=\"color-scheme\" content=\"light dark\" />\n  <style>\n    body {\n      margin: 0;\n      padding: 0;\n      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\n      background-color: #f4f4f4;\n      color: #333;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      height: 100vh;\n      text-align: center;\n    }\n\n    .container {\n      background: #fff;\n      padding: 2rem;\n      border-radius: 12px;\n      box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\n      width: 90%;\n      max-width: 400px;\n      position: relative;\n    }\n\n    h1 {\n      margin-bottom: 1rem;\n      font-size: 2rem;\n    }\n\n    input[type=\"color\"] {\n      width: 100px;\n      height: 100px;\n      margin: 1rem 0;\n      cursor: pointer;\n    }\n\n    ul {\n      text-align: left;\n      margin: 1rem 0;\n      padding-left: 1.5rem;\n    }\n\n    li {\n      font-size: 1rem;\n      color: #555;\n      line-height: 1.6;\n    }\n\n    #hexWrapper {\n      position: relative;\n      display: inline-flex;\n      align-items: center;\n      gap: 0.5rem;\n      margin-top: 0.5rem;\n    }\n\n    #hexDisplay {\n      font-family: monospace;\n      font-size: 1.25rem;\n      padding: 0.5rem 1rem;\n      background: #e0e0e0;\n      border-radius: 6px;\n      cursor: pointer;\n      display: inline-block;\n    }\n\n    #tooltip {\n      color: #555;\n      font-size: 0.9rem;\n      font-weight: 500;\n      white-space: nowrap;\n      opacity: 1;\n      transition: opacity 0.4s ease;\n      pointer-events: none;\n    }\n\n    #tooltip.hide {\n      opacity: 0;\n    }\n  </style>\n</head>\n<body>\n  <div class=\"container\">\n    <h1>Color Picker</h1>\n    <ul>\n      <li><strong>Step 1:</strong> Pick a color ‚Äî the hex code will appear below.</li>\n      <li><strong>Step 2:</strong> Tap the hex code to copy it to your clipboard.</li>\n      <li><strong>Step 3:</strong> Tap Done.</li>\n    </ul>\n    <input type=\"color\" id=\"colorInput\">\n    <div id=\"hexWrapper\">\n      <div id=\"hexDisplay\" tabindex=\"0\">#000000</div>\n      <div id=\"tooltip\">üìã Copy</div>\n    </div>\n  </div>\n\n  <script>\n    const colorInput = document.getElementById('colorInput');\n    const hexDisplay = document.getElementById('hexDisplay');\n    const tooltip = document.getElementById('tooltip');\n\n    function showTemporaryMessage(message) {\n      tooltip.textContent = message;\n      tooltip.classList.remove('hide');\n\n      setTimeout(() => {\n        tooltip.classList.add('hide');\n        setTimeout(() => {\n          tooltip.textContent = 'üìã Copy';\n          tooltip.classList.remove('hide');\n        }, 400);\n      }, 2000);\n    }\n\n    colorInput.addEventListener('input', () => {\n      const hex = colorInput.value.toUpperCase();\n      hexDisplay.textContent = hex;\n    });\n\n    hexDisplay.addEventListener('click', () => {\n      navigator.clipboard.writeText(hexDisplay.textContent).then(() => {\n        showTemporaryMessage('‚úîÔ∏è Copied');\n      });\n    });\n  </script>\n</body>\n</html>",
                "UUID": "451C8867-C101-4A41-A5A0-9715162D8395"
            }
        },
